/* pages/ordercenter/ordercenter.wxss */
page{
  background: #F3F3F3;
}
page,
.page,
.page__bd{
    height: 100%;
}
.page__bd{
    padding-bottom: 0;
}
.weui-tab__content{
    /* padding-top: 60px;
    text-align: center; */
}
.weui-tab {
position:relative;
height:100%;

}
.weui-navbar{
  position:absolute;
height: 90rpx;
  background: #FFFFFF;
z-index:500;
top:0;
width:100%;
/* border-bottom:1rpx solid #ccc; */
display:-webkit-box;
   box-shadow: 0 2rpx 4rpx #C4D7FA;

}
.weui-navbar__item{
  position:relative;
display:block;
-webkit-box-flex:1;
-webkit-flex:1;
flex:1;
padding:13px 0;
text-align:center;
font-size:0;

}
.weui-navbar__title{
  display:inline-block;
/* font-size:15px; */
max-width:8em;
width:auto;
overflow:hidden;
text-overflow:ellipsis;
white-space:nowrap;
word-wrap:normal;
font-family: PingFang SC;
font-weight: Regular;
font-size: 24rpx;
color: #999999;

}
.weui-tab__panel{
  box-sizing:border-box;
height:100%;
padding-top:50px;
overflow:auto;
-webkit-overflow-scrolling:touch;

}
.weui-tab__content {
 /* padding-top:60px;
text-align:center; */
 
}
.weui-bar__item_on view{
font-family: PingFang SC;
font-weight: Bold;
font-size: 32rpx;
color: #999999;
}
.weui-navbar__slider{
  position:absolute;
content:" ";
/* left:0; */
bottom:0;
width:126rpx;
height:4rpx;
background-color:#69B2FC;
-webkit-transition:-webkit-transform .3s;
transition:-webkit-transform .3s;

}
